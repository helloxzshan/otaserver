<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.otaserver.bean.FileInfo" table="file_info">
        <id name="fileId" type="java.lang.String">
            <column name="file_id" />
            <generator class="assigned" />
        </id>
        <property name="firmwareId" type="java.lang.String">
            <column name="firmware_id" length="2000000000" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="2000000000" />
        </property>
        <property name="local" type="java.lang.String">
            <column name="local" length="2000000000" />
        </property>
        <property name="md5" type="java.lang.String">
            <column name="md5" length="2000000000" />
        </property>
        <property name="url" type="java.lang.String">
            <column name="url" length="2000000000" />
        </property>
    </class>
</hibernate-mapping>
